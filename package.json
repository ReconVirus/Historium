        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-typescript": "^6.0.0",
        "@types/node": "^14.14.2",
        "obsidian": "latest",
        "rollup": "^2.32.1",
        "sass": "^1.66.1",
        "tslib": "^2.0.3",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "rollup-plugin-styles": "^4.0.0",
        "vis-timeline": "^7.7.2"
    }
